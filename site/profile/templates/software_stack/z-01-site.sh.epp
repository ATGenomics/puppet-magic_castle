#!/bin/bash

if [[ $UID -ge <%= $min_uid %> ]]; then
    export LMOD_SYSTEM_DEFAULT_MODULES="<%= $lmod_default_modules.join(' ') %>"

<% $extra_site_env_vars.each | $key, $value| { -%>
    export <%= $key %>=<%= $value %>
<% } -%>

    if [[ -r <%= $initial_profile %>  ]]; then
        source <%= $initial_profile %>
    fi
fi
